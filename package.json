{
  "name": "@tlib/package-template",
  "description": "an example package published from template for typescript libraries",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "tlib",
    "typescript",
    "semantic-release",
    "codegen",
    "template"
  ],
  "author": {
    "name": "EqualMa",
    "email": "equalma@outlook.com",
    "url": "https://github.com/EqualMa"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tlibjs/package-template.git"
  },
  "scripts": {
    "test": "jest",
    "test-ts-node": "ts-node scripts/test-ts-node.ts -s --help ./src/some.ts",
    "precommit": "lint-staged",
    "lint:fix": "eslint --cache --max-warnings 0 --fix",
    "ensure-linted": "eslint --max-warnings 0 .",
    "semantic-release": "semantic-release",
    "build": "yarn run clean && rollup -c --configPlugin typescript",
    "clean": "rimraf dist",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "@rollup/plugin-typescript": "^8.2.3",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@types/glob": "^7.1.4",
    "@types/jest": "^26.0.24",
    "@types/rimraf": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "babel-jest": "^27.0.6",
    "builtin-modules": "^3.2.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.1",
    "pascal-case": "^3.1.2",
    "prettier": "2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.53.2",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-node-externals": "^2.1.3",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^17.4.4",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "core-js": "^3.15.2",
    "pascal-case": "^3.1.2",
    "tslib": "^2.3.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "version": "1.2.0"
}
